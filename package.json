    "@bigtest/interactor": "^0.7.0",
    "@bigtest/mocha": "^0.5.1",
    "@bigtest/react": "^0.1.2",
    "@folio/eslint-config-stripes": "^5.3.0",
    "@folio/stripes": "^6.0.0",
