    "@folio/stripes-components": "^9.0.0",
    "@folio/stripes-connect": "^6.0.0",
    "@folio/stripes-core": "^7.0.0",
    "@folio/stripes-util": "^3.0.0",
    "@folio/stripes-smart-components": "^6.0.0",
